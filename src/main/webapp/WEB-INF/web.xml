<web-app>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.example.UserRegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.example.UserLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CreateContact</servlet-name>
        <servlet-class>com.example.CreateContactServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateContact</servlet-name>
        <url-pattern>/create</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UpdateUser</servlet-name>
        <servlet-class>com.example.EditServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateUser</servlet-name>
        <url-pattern>/update</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Prime</servlet-name>
        <servlet-class>com.example.primeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Prime</servlet-name>
        <url-pattern>/prime</url-pattern>
    </servlet-mapping>
</web-app>